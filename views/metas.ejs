<html>
<%
var itemselecionado = itens[0];

var obj = new Array(7);

obj[0] = new Array(itemselecionado.meses.length);
obj[1] = new Array(itemselecionado.meses.length);
obj[2] = new Array(itemselecionado.meses.length);
obj[3] = new Array(itemselecionado.meses.length);
obj[4] = new Array(itemselecionado.meses.length);
obj[5] = new Array(itemselecionado.meses.length);
obj[6] = new Array(itemselecionado.meses.length);

obj[0][0]='';
obj[1][0]='Realizado';
obj[2][0]='Quantitativo a ser realizado 2016';
obj[3][0]='Resultado Anual';
obj[4][0]='Quantitativo a ser realizado 2016-19';
obj[5][0]='Resultado Quadrienal';
obj[6][0]='Quantidade Acumulada 2016';

itemselecionado.meses.forEach((item,i) => {
   obj[0][i+1]=item.nome;
   obj[1][i+1]=item.realizado;  
   obj[2][i+1]=item.aRealizar;
   obj[3][i+1]=item.resultadoAnual;
   obj[4][i+1]=item.aRealizar19;
   obj[5][i+1]=item.resultadoQuadrienal;
   obj[6][i+1]=item.qtdAcumulada;
});

%>
<body>
    <table>
        <tr>
            <th><%=itemselecionado.sigla%> - <%=itemselecionado.nome%></th>
        </tr>
        <tr>
            <th>Ano <%=itemselecionado.ano%></th>
            <th>Linha de base <%=itemselecionado.linhaBase%></th>
            <th>Meta <%=itemselecionado.meta%></th>
            <th>Meta f&iacute;sica - Total <%=itemselecionado.metaQuadrienal%></th>
        </tr>
        <%for(j=0;j<obj.length;j++){%>
        <tr>
            <%for(i=0;i<itemselecionado.meses.length;i++){%>
            <td><%=obj[j][i]%></td>
            <%}%>
        </tr>
        <%}%>
    </table>
</body>
</html>